{
  "name": "bamflix-workers",
  "version": "1.0.0",
  "description": "Bam-flix Cambodia backend on Cloudflare Workers",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:init": "wrangler d1 execute bamflix-db --file=./schema.sql",
    "db:migrate": "wrangler d1 migrations apply bamflix-db"
  },
  "dependencies": {
    "hono": "^3.11.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.1"
  }
}